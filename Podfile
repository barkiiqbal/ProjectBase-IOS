# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

def shared_pods
    
    # Pods for Targets dev and pro
    pod 'Alamofire'
    pod 'MBProgressHUD'
    pod 'SwiftyJSON'
    pod 'ObjectMapper'
    pod 'Locksmith'
    
end

target 'ProjectBase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ProjectBase
  shared_pods
  
end

target 'ProjectBase-Dev' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for ProjectBase-Dev
    shared_pods
    
end
